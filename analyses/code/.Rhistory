n = nrow(component_1),
n2 = nrow(component_2),
r12 = cor.c1.spearman,
r34 = cor.c2.spearman)
rtest.pearson <- r.test(
n = nrow(component_1),
n2 = nrow(component_2),
r12 = cor.c1.pearson,
r34 = cor.c2.pearson)
